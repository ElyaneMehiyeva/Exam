
@model List<Team>
<div class="mb-4">
    <a >Add New Item</a>
</div>
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">DataTables Example</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>Image</th>
                        <th>FullName</th>
                        <th>Position</th>
                        <th>Description</th>
                        <th>Twitter</th>
                        <th>Facebook</th>
                        <th>Instagram</th>
                        <th>Linkedin</th>
                        <th></th>
                    </tr>
                </thead>
              
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>Tiger Nixon</td>
                            <td>System Architect</td>
                            <td>Edinburgh</td>
                            <td>61</td>
                            <td>2011/04/25</td>
                            <td>
                                <a class="btn btn-success" asp-controller="Team" asp-action="Edit">Edit </a>
                                <a class="btn btn-danger" asp-controller="Team" asp-action="Delete">Delete </a>
                            </td>
                        </tr>
                    }

                </tbody>
            </table>
        </div>
    </div>
</div>
